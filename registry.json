{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "connect-game-registry",
  "homepage": "http://localhost:5174",
  "items": [
    {
      "name": "connect-game",
      "type": "registry:block",
      "title": "Connect Game",
      "description": "Mini igra zasnovana na React + react-konva, sa prilagoÄ‘enim hookovima i assetima.",
